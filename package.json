{
	"name": "json-to-java-stack-trace",
	"displayName": "json to java stack-trace",
	"description": "Convert a Json Exception to java stack-trace",
	"publisher" : "pedrolopixn",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/pedrolopix/json-to-java-stack-trace.git"
	  },
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:json-to-java-stack-trace.toStackTrace"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "json-to-java-stack-trace.toStackTrace",
				"title": "Convert Json to Stack Trace"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js",
		"install": "vsce package  && code --install-extension json-to-java-stack-trace-0.0.1.vsix"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	}
}
